{
  "name": "beegram",
  "version": "0.3.0",
  "description": "MTProto API Client Framework",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && npm run generate && tsc",
    "clean": "rm -rf ./dist && rm ./src/TL/Schema.ts ./src/TL/Proto.ts",
    "generate": "ts-node ./src/Scripts/generate-proto.ts && ts-node ./src/Scripts/generate-schema.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmzan/beegram.git"
  },
  "keywords": [
    "mtproto",
    "api",
    "client",
    "framework"
  ],
  "author": "Ahmzan <ahmzan.dev@gmail.com>",
  "license": "LGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/ahmzan/beegram/issues"
  },
  "homepage": "https://github.com/ahmzan/beegram#readme",
  "dependencies": {
    "@cryptography/aes": "^0.1.1",
    "better-sqlite3": "^7.4.3",
    "big-integer": "^1.6.48",
    "bignumber.js": "^9.0.1",
    "debug": "^4.3.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.3",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "jest": "^27.0.5",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
